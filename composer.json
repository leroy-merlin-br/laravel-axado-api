    "keywords": ["axado","laravel","shipping", "brazil", "laravel","cep","quotation","frete","brasil"],
    "license": "MIT",
    "authors": [
        {
            "name": "Guilherme Guitte",
            "email": "guilherme.guitte@gmail.com"
        },
        {
            "name": "Zizaco Zizuini",
            "email": "zizaco@gmail.com"
        },
        {
            "name": "Ricardo Plansky",
            "email": "ricardo.plansky@yahoo.com"
        }
